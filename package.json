{
  "name": "meta-editor",
  "version": "0.0.1",
  "main": "./dist/electron/electron.js",
  "type": "module",
  "scripts": {
    "compile-electron": "tsc && renamer --force --find .js --replace .mjs ./dist/electron/preload*",
    "run-electron": "electron .",
    "start-electron": "run-s compile-electron run-electron",
    "build-electron": "run-s build start-electron",
    "dev-electron": "run-p dev start-electron",
    "dev": "vite dev",
    "build": "vite build && tsc",
    "web-preview": "vite build && vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --max-warnings 0 './src/**/*.{ts,js,svelte}'",
    "lint:fix": "eslint --max-warnings 0 './src/**/*.{ts,js,svelte}' --fix"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@types/pako": "^2.0.0",
    "@types/semver": "^7.5.4",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "electron": "^28.1.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-svelte": "^2.34.0",
    "npm-run-all": "^4.1.5",
    "phosphor-svelte": "^1.4.2",
    "postcss": "^8.4.31",
    "renamer": "^4.0.0",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^4.5.0"
  },
  "dependencies": {
    "@meta-system/object-definition": "^2.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "json-beautify": "^1.1.1",
    "just-clone": "^6.2.0",
    "jwt-decode": "^4.0.0",
    "meta-system": "^0.4.5",
    "nanoid": "^5.0.2",
    "pako": "^2.1.0",
    "sass": "^1.49.8",
    "semver": "^7.5.4"
  }
}
